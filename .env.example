# Example configuration for environment variables
# Copy this to .env and fill in your actual API keys

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Configuration (alternative to OpenAI)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini Configuration (another alternative)
GOOGLE_API_KEY=your_google_api_key_here

# AI Model Selection (choose one)
# AI_MODEL=gpt-4              # For OpenAI
# AI_MODEL=claude-3-sonnet    # For Anthropic
# AI_MODEL=gemini-pro         # For Google Gemini
AI_MODEL=gemini-pro

# Browser Configuration
BROWSER_HEADLESS=false
BROWSER_TIMEOUT=30

# Terminal Configuration
TERMINAL_TIMEOUT=30

# Logging Level
LOG_LEVEL=INFO

# Session Configuration
MAX_ITERATIONS=3
